[{"name": "The German Woman", "number": 1, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109969"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/109969/foyles-war-1x01-the-german-woman", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/12/30187.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/12/30187.jpg"}, "id": 109969, "airstamp": "2002-10-27T20:00:00+00:00", "summary": "<p>The German wife of a rich and influential aristocrat exempted from wartime internment is found decapitated in the midst of anti-German feeling.</p>", "runtime": 120, "airdate": "2002-10-27", "airtime": "20:00"}, {"name": "The White Feather", "number": 2, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109970"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/109970/foyles-war-1x02-the-white-feather", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/12/30188.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/12/30188.jpg"}, "id": 109970, "airstamp": "2002-11-03T20:00:00+00:00", "summary": "<p>With the English army trapped in France and a German invasion imminent, a Nazi sympathizer is murdered during a pro-Facist meeting at a country hotel.</p>", "runtime": 120, "airdate": "2002-11-03", "airtime": "20:00"}, {"name": "A Lesson In Murder", "number": 3, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109971"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/109971/foyles-war-1x03-a-lesson-in-murder", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/12/30189.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/12/30189.jpg"}, "id": 109971, "airstamp": "2002-11-10T20:00:00+00:00", "summary": "<p>Foyle investigates the suicide of a pacifist and death threats to the judge who ruled against his conscientious objector status.</p>", "runtime": 120, "airdate": "2002-11-10", "airtime": "20:00"}, {"name": "Eagle Day", "number": 4, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109972"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/109972/foyles-war-1x04-eagle-day", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/12/30190.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/12/30190.jpg"}, "id": 109972, "airstamp": "2002-11-17T20:00:00+00:00", "summary": "<p>Foyle investigates a man found with a knife in his stomach in a bombed out building while son Andrew gets involved in a top secret radar program.</p>", "runtime": 120, "airdate": "2002-11-17", "airtime": "20:00"}, {"name": "Fifty Ships", "number": 1, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109973"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/109973/foyles-war-2x01-fifty-ships", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/12/30192.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/12/30192.jpg"}, "id": 109973, "airstamp": "2003-11-16T20:00:00+00:00", "summary": "<p>After Foyle is a guest at a friend's home at a dinner for an important American diplomat, a suicide victim and German agent both are discovered near the house.</p>", "runtime": 120, "airdate": "2003-11-16", "airtime": "20:00"}, {"name": "Among The Few", "number": 2, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109974"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/109974/foyles-war-2x02-among-the-few", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/12/30193.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/12/30193.jpg"}, "id": 109974, "airstamp": "2003-11-23T20:00:00+00:00", "summary": "<p>Sam volunteers to go undercover to discover how rationed gasoline is being stolen from a fuel depot, and Andrew becomes a suspect in a murder case.</p>", "runtime": 120, "airdate": "2003-11-23", "airtime": "20:00"}, {"name": "War Games", "number": 3, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109975"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/109975/foyles-war-2x03-war-games", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/12/30194.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/12/30194.jpg"}, "id": 109975, "airstamp": "2003-11-30T20:00:00+00:00", "summary": "<p>When a British food corporation signs a secret agreement with the Nazis to provide essential food services to the enemy during wartime, murders result.</p>", "runtime": 120, "airdate": "2003-11-30", "airtime": "20:00"}, {"name": "The Funk Hole", "number": 4, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109976"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/109976/foyles-war-2x04-the-funk-hole", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/12/30195.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/12/30195.jpg"}, "id": 109976, "airstamp": "2003-12-07T20:00:00+00:00", "summary": "<p>Foyle's investigation of the death of a young black marketeer and stolen food is interrupted by his suspension on charges of sedition.</p>", "runtime": 120, "airdate": "2003-12-07", "airtime": "20:00"}, {"name": "The French Drop", "number": 1, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109977"}}, "season": 3, "url": "http://www.tvmaze.com/episodes/109977/foyles-war-3x01-the-french-drop", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/12/30196.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/12/30196.jpg"}, "id": 109977, "airstamp": "2004-10-24T20:00:00+01:00", "summary": "<p>February 1941: Foyle again pursues a position that would allow him to contribute more to the war effort. But a local murder investigation sidetracks this endeavor as Foyle finds himself caught between rival spy organizations.</p>", "runtime": 120, "airdate": "2004-10-24", "airtime": "20:00"}, {"name": "Enemy Fire", "number": 2, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109978"}}, "season": 3, "url": "http://www.tvmaze.com/episodes/109978/foyles-war-3x02-enemy-fire", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/12/30198.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/12/30198.jpg"}, "id": 109978, "airstamp": "2004-10-31T20:00:00+00:00", "summary": "<p>February 1941: When a local manor house is commandeered for use as a special burn unit for treating injured RAF pilots, Foyle is called in to investigate a series of strange accidents.</p>", "runtime": 120, "airdate": "2004-10-31", "airtime": "20:00"}, {"name": "They Fought in the Fields", "number": 3, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109979"}}, "season": 3, "url": "http://www.tvmaze.com/episodes/109979/foyles-war-3x03-they-fought-in-the-fields", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/12/30199.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/12/30199.jpg"}, "id": 109979, "airstamp": "2004-11-07T20:00:00+00:00", "summary": "<p>April 1941: Foyle questions three Land Army girls about a murder that occurs on a Hastings farm.</p>", "runtime": 120, "airdate": "2004-11-07", "airtime": "20:00"}, {"name": "War of Nerves", "number": 4, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109980"}}, "season": 3, "url": "http://www.tvmaze.com/episodes/109980/foyles-war-3x04-war-of-nerves", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/12/30200.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/12/30200.jpg"}, "id": 109980, "airstamp": "2004-11-14T20:00:00+00:00", "summary": "<p>A.C. Rose orders Foyle to delegate his investigation of black marketing ring and personally look into the possibly seditious acts of a Socialist activist.</p>", "runtime": 120, "airdate": "2004-11-14", "airtime": "20:00"}, {"name": "Invasion", "number": 1, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109981"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/109981/foyles-war-4x01-invasion", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/12/30202.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/12/30202.jpg"}, "id": 109981, "airstamp": "2006-01-15T20:00:00+00:00", "summary": "<p>American troops arrive in Britain and the Corps of Engineers building an airstrip on a Hastings farm is met with resentment.</p>", "runtime": 120, "airdate": "2006-01-15", "airtime": "20:00"}, {"name": "Bad Blood", "number": 2, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109982"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/109982/foyles-war-4x02-bad-blood", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/12/30203.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/12/30203.jpg"}, "id": 109982, "airstamp": "2006-01-22T20:00:00+00:00", "summary": "<p>Foyle investigates the a mysterious disease affecting local farm animals and looks into the arrest of a pacifist accused of stabbing a war hero.</p>", "runtime": 120, "airdate": "2006-01-22", "airtime": "20:00"}, {"name": "Bleak Midwinter", "number": 1, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109983"}}, "season": 5, "url": "http://www.tvmaze.com/episodes/109983/foyles-war-5x01-bleak-midwinter", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/12/30205.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/12/30205.jpg"}, "id": 109983, "airstamp": "2007-02-11T20:00:00+00:00", "summary": "<p>The apparently accidental death of a young woman in a munitions factory becomes linked with the murder of Milner's estranged wife as he falls under suspicion.</p>", "runtime": 120, "airdate": "2007-02-11", "airtime": "20:00"}, {"name": "Casualties OF War", "number": 2, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109984"}}, "season": 5, "url": "http://www.tvmaze.com/episodes/109984/foyles-war-5x02-casualties-of-war", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/12/30206.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/12/30206.jpg"}, "id": 109984, "airstamp": "2007-04-15T20:00:00+01:00", "summary": "<p>Foyle has his hands full dealing with illegal gambling, sabotage, and his needy goddaughter who shows up on his doorstep with her traumatized son.</p>", "runtime": 120, "airdate": "2007-04-15", "airtime": "20:00"}, {"name": "Plan of Attack", "number": 1, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109985"}}, "season": 6, "url": "http://www.tvmaze.com/episodes/109985/foyles-war-6x01-plan-of-attack", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/12/30207.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/12/30207.jpg"}, "id": 109985, "airstamp": "2008-01-06T20:00:00+00:00", "summary": "<p>Milner is unhappy with his new DCS and has asked for a transfer, but after he is murdered, Foyle comes out of retirement to replace him.</p>", "runtime": 120, "airdate": "2008-01-06", "airtime": "20:00"}, {"name": "Broken Souls", "number": 2, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109986"}}, "season": 6, "url": "http://www.tvmaze.com/episodes/109986/foyles-war-6x02-broken-souls", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/12/30208.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/12/30208.jpg"}, "id": 109986, "airstamp": "2008-04-13T20:00:00+01:00", "summary": "<p>Foyles hones in on two murders: an unpopular psychiatrist in a trauma facility and a low security German POW on work release to a local farm.</p>", "runtime": 120, "airdate": "2008-04-13", "airtime": "20:00"}, {"name": "All Clear", "number": 3, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109987"}}, "season": 6, "url": "http://www.tvmaze.com/episodes/109987/foyles-war-6x03-all-clear", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/12/30210.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/12/30210.jpg"}, "id": 109987, "airstamp": "2008-04-20T20:00:00+01:00", "summary": "<p>Foyle joins a municipal committee that's preparing for the now imminent V-E Day celebration but soon finds himself investigating the deaths of two of its members.</p>", "runtime": 120, "airdate": "2008-04-20", "airtime": "20:00"}, {"name": "The Russian House", "number": 1, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109988"}}, "season": 7, "url": "http://www.tvmaze.com/episodes/109988/foyles-war-7x01-the-russian-house", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/17/42956.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/17/42956.jpg"}, "id": 109988, "airstamp": "2010-04-11T20:00:00+01:00", "summary": "<p>It's June 1945 and Detective Chief Superintendent Foyle is still on duty as they are unable to find a replacement for him. He's approached by Brigadier Timothy Wilson, his former commanding officer from his own military service in World War I, who is looking for a Russian POW, Ivan Spiakov, who has escaped and is believed to have been seen in the Hastings area.</p>", "runtime": 120, "airdate": "2010-04-11", "airtime": "20:00"}, {"name": "Killing Time", "number": 2, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109989"}}, "season": 7, "url": "http://www.tvmaze.com/episodes/109989/foyles-war-7x02-killing-time", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/17/44123.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/17/44123.jpg"}, "id": 109989, "airstamp": "2010-04-18T20:00:00+01:00", "summary": "<p>The disappearance of a Foreign Office official appears to explain the deaths of a number of Russian agents working for British Intelligence.</p>", "runtime": 120, "airdate": "2010-04-18", "airtime": "20:00"}, {"name": "The Hide", "number": 3, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109990"}}, "season": 7, "url": "http://www.tvmaze.com/episodes/109990/foyles-war-7x03-the-hide", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/18/46534.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/18/46534.jpg"}, "id": 109990, "airstamp": "2010-04-25T20:00:00+01:00", "summary": "<p>Foyle battles to save a young man accused of high treason from the executioner's noose in a case that will shatter his personal world to the core.</p>", "runtime": 120, "airdate": "2010-04-25", "airtime": "20:00"}, {"name": "The Eternity Ring", "number": 1, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109991"}}, "season": 8, "url": "http://www.tvmaze.com/episodes/109991/foyles-war-8x01-the-eternity-ring", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/34/85398.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/34/85398.jpg"}, "id": 109991, "airstamp": "2013-03-24T20:00:00+00:00", "summary": "<p>As Foyle returns from a stay in the USA, MI5 agents persuade him to go to their London HQ immediately.</p>", "runtime": 120, "airdate": "2013-03-24", "airtime": "20:00"}, {"name": "The Cage", "number": 2, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109992"}}, "season": 8, "url": "http://www.tvmaze.com/episodes/109992/foyles-war-8x02-the-cage", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/34/86091.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/34/86091.jpg"}, "id": 109992, "airstamp": "2013-03-31T20:00:00+01:00", "summary": "<p>The disappearance of a Foreign Office official appears to explain the deaths of a number of Russian agents working for British Intelligence.</p>", "runtime": 120, "airdate": "2013-03-31", "airtime": "20:00"}, {"name": "Sunflower", "number": 3, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109993"}}, "season": 8, "url": "http://www.tvmaze.com/episodes/109993/foyles-war-8x03-sunflower", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/34/86404.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/34/86404.jpg"}, "id": 109993, "airstamp": "2013-04-07T20:00:00+01:00", "summary": "<p>Foyle is told by the head of MI5 that eminent art historian Professor Van Haren is actually a high-ranking Nazi whose real name is Karl Strasser.</p>", "runtime": 120, "airdate": "2013-04-07", "airtime": "20:00"}, {"name": "High Castle", "number": 1, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109994"}}, "season": 9, "url": "http://www.tvmaze.com/episodes/109994/foyles-war-9x01-high-castle", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/44/110216.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/44/110216.jpg"}, "id": 109994, "airstamp": "2015-01-04T20:00:00+00:00", "summary": "<p>Foyle, now working for British Intelligence, is drawn into the world of corrupt Nazi businessmen when London university professor William Knowles is found brutally murdered in a London park.</p>", "runtime": 120, "airdate": "2015-01-04", "airtime": "20:00"}, {"name": "Trespass", "number": 2, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109995"}}, "season": 9, "url": "http://www.tvmaze.com/episodes/109995/foyles-war-9x02-trespass", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/44/110439.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/44/110439.jpg"}, "id": 109995, "airstamp": "2015-01-11T20:00:00+00:00", "summary": "<p>When a young student is badly assaulted, Foyle wonders if the attack is racially motivated as the young man was the son of a wealthy and high profile Jewish businessman. Sam is determined to help a young boy in Adam's constituency.</p>", "runtime": 120, "airdate": "2015-01-11", "airtime": "20:00"}, {"name": "Elise", "number": 3, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/109996"}}, "season": 9, "url": "http://www.tvmaze.com/episodes/109996/foyles-war-9x03-elise", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/44/110881.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/44/110881.jpg"}, "id": 109996, "airstamp": "2015-01-18T20:00:00+00:00", "summary": "<p>When his colleague Hilda Pierce is nearly killed in an attempted assassination, Foyle must re-examine her top-secret role.</p>", "runtime": 120, "airdate": "2015-01-18", "airtime": "20:00"}]
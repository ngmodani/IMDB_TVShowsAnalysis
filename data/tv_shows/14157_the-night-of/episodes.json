[{"name": "The Beach", "number": 1, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/767996"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/767996/the-night-of-1x01-the-beach", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/64/160090.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/64/160090.jpg"}, "id": 767996, "airstamp": "2016-07-10T21:00:00-04:00", "summary": "<p>A perfect night for a Pakistani-American student turns into a nightmare when he is arrested for murder.</p>", "runtime": 90, "airdate": "2016-07-10", "airtime": "21:00"}, {"name": "Subtle Beast", "number": 2, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/845771"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/845771/the-night-of-1x02-subtle-beast", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/67/168229.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/67/168229.jpg"}, "id": 845771, "airstamp": "2016-07-17T21:00:00-04:00", "summary": "<p>As attorney John Stone counsels Naz, lead detective Dennis Box investigates the crime.</p>", "runtime": 60, "airdate": "2016-07-17", "airtime": "21:00"}, {"name": "A Dark Crate", "number": 3, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/845772"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/845772/the-night-of-1x03-a-dark-crate", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/68/171843.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/68/171843.jpg"}, "id": 845772, "airstamp": "2016-07-24T21:00:00-04:00", "summary": "<p>Awaiting his formal arraignment at Rikers Island, Naz realizes that his survival, or perhaps his demise, rests with a particular inmate, Freddy.</p>", "runtime": 60, "airdate": "2016-07-24", "airtime": "21:00"}, {"name": "The Art of War", "number": 4, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/845773"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/845773/the-night-of-1x04-the-art-of-war", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/68/171715.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/68/171715.jpg"}, "id": 845773, "airstamp": "2016-07-31T21:00:00-04:00", "summary": "<p>At Rikers, an inmate named Calvin takes Naz into his confidence as Freddy continues to make protection overtures. On the outside looking in, Stone plays detective and uncovers some important information about Andrea's troubled past. Alison and Helen discuss a possible plea deal for Naz as Chandra counsels the accused</p>", "runtime": 60, "airdate": "2016-07-31", "airtime": "21:00"}, {"name": "The Season of the Witch", "number": 5, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/845774"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/845774/the-night-of-1x05-the-season-of-the-witch", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/70/175618.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/70/175618.jpg"}, "id": 845774, "airstamp": "2016-08-07T21:00:00-04:00", "summary": "<p>Stone\n takes on a commission and rescues a detainee on death row. Naz is \npulled further into Freddy's sphere of influence at Rikers. Box \nreconstructs Naz's whereabouts the night of the murder, while Helen maps\n out a prosecution strategy. </p>", "runtime": 60, "airdate": "2016-08-07", "airtime": "21:00"}, {"name": "Samson and Delilah", "number": 6, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/845775"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/845775/the-night-of-1x06-samson-and-delilah", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/71/177865.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/71/177865.jpg"}, "id": 845775, "airstamp": "2016-08-14T21:00:00-04:00", "summary": "<p>As prosecutor Helen Weiss prepares for trial, Naz's alliance with Freddy deepens.</p>", "runtime": 60, "airdate": "2016-08-14", "airtime": "21:00"}, {"name": "Ordinary Death", "number": 7, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/845776"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/845776/the-night-of-1x07-ordinary-death", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/71/179994.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/71/179994.jpg"}, "id": 845776, "airstamp": "2016-08-21T21:00:00-04:00", "summary": "<p>The defense presents its case in the trial of The State v. Nasir Khan.</p>", "runtime": 60, "airdate": "2016-08-21", "airtime": "21:00"}, {"name": "The Call of the Wild", "number": 8, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/845777"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/845777/the-night-of-1x08-the-call-of-the-wild", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/72/181796.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/72/181796.jpg"}, "id": 845777, "airstamp": "2016-08-28T21:00:00-04:00", "summary": "<p> Stone finds himself in the spotlight thanks to a controversy enveloping Naz's defense team.</p>", "runtime": 96, "airdate": "2016-08-28", "airtime": "21:00"}]
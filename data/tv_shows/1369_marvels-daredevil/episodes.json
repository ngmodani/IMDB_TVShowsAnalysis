[{"name": "Into the Ring", "number": 1, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/117670"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/117670/marvels-daredevil-1x01-into-the-ring", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/10/25293.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/10/25293.jpg"}, "id": 117670, "airstamp": "2015-04-10T12:00:00+00:00", "summary": "<p>Matt Murdock, a Marvel Comics hero who's a lawyer by day but fights the bad guys at night to avenge the murder of his father, a boxer. But what sets Matt apart from most superheroes isn't his back story it's his handicap. Matt is blind, the result of a childhood accident that may have removed his ability to see but gifted him with heightened senses of touch, taste, hearing and smell.</p>", "runtime": 60, "airdate": "2015-04-10", "airtime": ""}, {"name": "Cut Man", "number": 2, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/117671"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/117671/marvels-daredevil-1x02-cut-man", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/10/25294.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/10/25294.jpg"}, "id": 117671, "airstamp": "2015-04-10T12:00:00+00:00", "summary": "<p>As Claire nurses Matt back to health, she discovers that her patient is much more than an ordinary man. Meanwhile, Matt recalls the events that led to him becoming Daredevil.</p>", "runtime": 60, "airdate": "2015-04-10", "airtime": ""}, {"name": "Rabbit in a Snowstorm", "number": 3, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/117672"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/117672/marvels-daredevil-1x03-rabbit-in-a-snowstorm", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/10/25295.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/10/25295.jpg"}, "id": 117672, "airstamp": "2015-04-10T12:00:00+00:00", "summary": "<p>Murdock and Foggy take on a mysterious wealthy client, but Murdock is convinced that there's more to the case than just the facts.</p>", "runtime": 60, "airdate": "2015-04-10", "airtime": ""}, {"name": "In the Blood", "number": 4, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/117673"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/117673/marvels-daredevil-1x04-in-the-blood", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/10/25296.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/10/25296.jpg"}, "id": 117673, "airstamp": "2015-04-10T12:00:00+00:00", "summary": "<p>Daredevil goes after the Russians who have abducted Claire. Meanwhile, Wilson goes on a dinner date with Vanessa.</p>", "runtime": 60, "airdate": "2015-04-10", "airtime": ""}, {"name": "World on Fire", "number": 5, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/117674"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/117674/marvels-daredevil-1x05-world-on-fire", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/10/25297.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/10/25297.jpg"}, "id": 117674, "airstamp": "2015-04-10T12:00:00+00:00", "summary": "<p>Fisk moves forward with plans that threaten to rip Hell's Kitchen apart. Murdock and Foggy take on a case helping tenants victimized by a slumlord.</p>", "runtime": 60, "airdate": "2015-04-10", "airtime": ""}, {"name": "Condemned", "number": 6, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/117675"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/117675/marvels-daredevil-1x06-condemned", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/10/25298.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/10/25298.jpg"}, "id": 117675, "airstamp": "2015-04-10T12:00:00+00:00", "summary": "<p>Daredevil finds himself trapped in the fallout of Fisk's plan to take control of Hell's Kitchen. Ben Urich digs closer to the truth.</p>", "runtime": 60, "airdate": "2015-04-10", "airtime": ""}, {"name": "Stick", "number": 7, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/117676"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/117676/marvels-daredevil-1x07-stick", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/10/25306.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/10/25306.jpg"}, "id": 117676, "airstamp": "2015-04-10T12:00:00+00:00", "summary": "<p>An important figure from Murdock's past reemerges seeking his help to battle a new enemy threatening Hell's Kitchen.</p>", "runtime": 60, "airdate": "2015-04-10", "airtime": ""}, {"name": "Shadows in the Glass", "number": 8, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/117677"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/117677/marvels-daredevil-1x08-shadows-in-the-glass", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/10/25308.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/10/25308.jpg"}, "id": 117677, "airstamp": "2015-04-10T12:00:00+00:00", "summary": "<p>While Murdock, Foggy and Karen's mission becomes clearer, Fisk's world spins further out of control in his battle for Hell's Kitchen.</p>", "runtime": 60, "airdate": "2015-04-10", "airtime": ""}, {"name": "Speak of the Devil", "number": 9, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/117678"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/117678/marvels-daredevil-1x09-speak-of-the-devil", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/10/25332.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/10/25332.jpg"}, "id": 117678, "airstamp": "2015-04-10T12:00:00+00:00", "summary": "<p>When Fisk gains the upper hand, the goal to destroy him becomes even more difficult, while Daredevil faces his own demons.</p>", "runtime": 60, "airdate": "2015-04-10", "airtime": ""}, {"name": "Nelson v. Murdock", "number": 10, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/117679"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/117679/marvels-daredevil-1x10-nelson-v-murdock", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/10/25333.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/10/25333.jpg"}, "id": 117679, "airstamp": "2015-04-10T12:00:00+00:00", "summary": "<p>Murdock and Foggy's relationship is put to the ultimate test while a new enemy against Fisk emerges.</p>", "runtime": 60, "airdate": "2015-04-10", "airtime": ""}, {"name": "The Path of the Righteous", "number": 11, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/117680"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/117680/marvels-daredevil-1x11-the-path-of-the-righteous", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/10/25334.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/10/25334.jpg"}, "id": 117680, "airstamp": "2015-04-10T12:00:00+00:00", "summary": "<p>Fisk and Murdock wrestle with the consequences of their chosen paths, while Ben and Karen get closer to Fisk's true past. </p>", "runtime": 60, "airdate": "2015-04-10", "airtime": ""}, {"name": "The Ones We Leave Behind", "number": 12, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/117681"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/117681/marvels-daredevil-1x12-the-ones-we-leave-behind", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/10/25335.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/10/25335.jpg"}, "id": 117681, "airstamp": "2015-04-10T12:00:00+00:00", "summary": "<p>Fisk seeks revenge while Karen is haunted by recent events. Daredevil makes a startling discovery about Fisk\\'s financing.</p>", "runtime": 60, "airdate": "2015-04-10", "airtime": ""}, {"name": "Daredevil", "number": 13, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/117682"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/117682/marvels-daredevil-1x13-daredevil", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/10/25299.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/10/25299.jpg"}, "id": 117682, "airstamp": "2015-04-10T12:00:00+00:00", "summary": "<p>In the season finale, a boxed-in Fisk and a desperate Murdock, Foggy and Karen are forced to play their end games.</p>", "runtime": 60, "airdate": "2015-04-10", "airtime": ""}, {"name": "Bang", "number": 1, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/558708"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/558708/marvels-daredevil-2x01-bang", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/49/122847.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/49/122847.jpg"}, "id": 558708, "airstamp": "2016-03-18T12:00:00+00:00", "summary": "<p>The New York City underworld is up for grabs after Wilson goes to jail. A new party arrives in town to kill all of the mobsters, and Daredevil goes after them.</p>", "runtime": 48, "airdate": "2016-03-18", "airtime": ""}, {"name": "Dogs to a Gunfight", "number": 2, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/559235"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/559235/marvels-daredevil-2x02-dogs-to-a-gunfight", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/49/122848.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/49/122848.jpg"}, "id": 559235, "airstamp": "2016-03-18T12:00:00+00:00", "summary": "<p>Matt recovers from the Punisher's head shot, while Foggy and Karen arrange a deal with DA Samantha Reyes to get their client Grotto into WITSEC. However, Reyes needs Grotto to do one thing before she agrees.</p>", "runtime": 50, "airdate": "2016-03-18", "airtime": ""}, {"name": "New York's Finest", "number": 3, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/559236"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/559236/marvels-daredevil-2x03-new-yorks-finest", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/49/122849.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/49/122849.jpg"}, "id": 559236, "airstamp": "2016-03-18T12:00:00+00:00", "summary": "<p>Punisher has a discussion with a captive Daredevil about what they do and why. Meanwhile, Reyes threatens to shut down Nelson &amp; Murdoch, and Karen sets out to get something on the DA. At the hospital ER, Foggy turns to Claire for help locating a missing Matt.</p>", "runtime": 48, "airdate": "2016-03-18", "airtime": ""}, {"name": "Penny and Dime", "number": 4, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/559237"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/559237/marvels-daredevil-2x04-penny-and-dime", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/49/122850.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/49/122850.jpg"}, "id": 559237, "airstamp": "2016-03-18T12:00:00+00:00", "summary": "<p>An Irish mobster seeking revenge for Punisher killing his son--and the $1.2 million that Punisher stole--arrives in Hell's Kitchen. Meanwhile, Karen gets a lead on Frank Castle and investigates his connection to the homicidal vigilante prowling the city.</p>", "runtime": 60, "airdate": "2016-03-18", "airtime": ""}, {"name": "Kinbaku", "number": 5, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/559238"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/559238/marvels-daredevil-2x05-kinbaku", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/49/122851.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/49/122851.jpg"}, "id": 559238, "airstamp": "2016-03-18T12:00:00+00:00", "summary": "<p>While Matt deals with Elektra in the present, he remembers what happened between them ten years ago. Meanwhile, Blake pressures Foggy for information on the Grote case, and Karen digs into Frank's background when she suspects coverup.</p>", "runtime": 56, "airdate": "2016-03-18", "airtime": ""}, {"name": "Regrets Only", "number": 6, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/559239"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/559239/marvels-daredevil-2x06-regrets-only", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/49/122852.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/49/122852.jpg"}, "id": 559239, "airstamp": "2016-03-18T12:00:00+00:00", "summary": "<p>Elektra convinces Matt to let her help eliminate the Yakuza from New York City. Meanwhile, Nelson &amp; Murdoch agree to enter a plea for Frank, and Karen learns more about their new client.</p>", "runtime": 56, "airdate": "2016-03-18", "airtime": ""}, {"name": "Semper Fidelis", "number": 7, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/559240"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/559240/marvels-daredevil-2x07-semper-fidelis", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/49/122853.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/49/122853.jpg"}, "id": 559240, "airstamp": "2016-03-18T12:00:00+00:00", "summary": "<p>Foggy tries to defend Frank in court, despite Matt's continuing absence as Daredevil teams up with Elektra to find out what the Japanese are up to.</p>", "runtime": 56, "airdate": "2016-03-18", "airtime": ""}, {"name": "Guilty as Sin", "number": 8, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/559241"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/559241/marvels-daredevil-2x08-guilty-as-sin", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/49/122854.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/49/122854.jpg"}, "id": 559241, "airstamp": "2016-03-18T12:00:00+00:00", "summary": "<p>Foggy tries to mount Frank's legal defense despite Matt's ongoing absence. Meanwhile, Matt is reunited with an old friend and discovers his connection to Elektra... and the secret of the Hand.</p>", "runtime": 60, "airdate": "2016-03-18", "airtime": ""}, {"name": "Seven Minutes in Heaven", "number": 9, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/559242"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/559242/marvels-daredevil-2x09-seven-minutes-in-heaven", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/49/122855.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/49/122855.jpg"}, "id": 559242, "airstamp": "2016-03-18T12:00:00+00:00", "summary": "<p>Wilson arranges for Frank to meet him in prison. Meanwhile, Foggy tries to keep the law firm afloat but discovers that Matt isn't interested.</p>", "runtime": 60, "airdate": "2016-03-18", "airtime": ""}, {"name": "The Man in the Box", "number": 10, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/559243"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/559243/marvels-daredevil-2x10-the-man-in-the-box", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/49/122856.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/49/122856.jpg"}, "id": 559243, "airstamp": "2016-03-18T12:00:00+00:00", "summary": "<p>People connected to the Central Park massacre are brutally gunned down, but Karen suspects that someone is setting Punisher up for a fall. Meanwhile, Matt convinces Claire to look after the Hand prisoners, but it turns out that they may not be as helpless as they appear.</p>", "runtime": 60, "airdate": "2016-03-18", "airtime": ""}, {"name": ".380", "number": 11, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/559244"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/559244/marvels-daredevil-2x11-380", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/49/122857.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/49/122857.jpg"}, "id": 559244, "airstamp": "2016-03-18T12:00:00+00:00", "summary": "<p>Karen escapes her protective custody and meets with Frank, who is on the trail of Blacksmith. Meanwhile, Daredevil's own search for the elusive drug lord takes him to Madame Gao, who warns that his quest won't end well.</p>", "runtime": 60, "airdate": "2016-03-18", "airtime": ""}, {"name": "The Dark at the End of the Tunnel", "number": 12, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/559245"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/559245/marvels-daredevil-2x12-the-dark-at-the-end-of-the-tunnel", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/49/122858.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/49/122858.jpg"}, "id": 559245, "airstamp": "2016-03-18T12:00:00+00:00", "summary": "<p>After the Hand abduct Stick, Daredevil traces them to their lair beneath the city. Meanwhile, Karen continues her investigation into Frank's background after his death.</p>", "runtime": 60, "airdate": "2016-03-18", "airtime": ""}, {"name": "A Cold Day in Hell's Kitchen", "number": 13, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/559246"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/559246/marvels-daredevil-2x13-a-cold-day-in-hells-kitchen", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/49/122859.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/49/122859.jpg"}, "id": 559246, "airstamp": "2016-03-18T12:00:00+00:00", "summary": "<p>Daredevil battles the Hand--with Elektra at his side--and discovers that the price of victory may prove too high to accept.</p>", "runtime": 60, "airdate": "2016-03-18", "airtime": ""}]
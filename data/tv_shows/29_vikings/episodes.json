[{"name": "Rites of Passage", "number": 1, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1547"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/1547/vikings-1x01-rites-of-passage", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/8/21463.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/8/21463.jpg"}, "id": 1547, "airstamp": "2013-03-03T22:00:00-04:00", "summary": "<p>A Viking warrior seeks approval from his community's leader in the opener of this series chronicling the medieval adventures of a band of Norsemen.</p>", "runtime": 60, "airdate": "2013-03-03", "airtime": "22:00"}, {"name": "Wrath of the Northmen", "number": 2, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1548"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/1548/vikings-1x02-wrath-of-the-northmen", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/8/21464.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/8/21464.jpg"}, "id": 1548, "airstamp": "2013-03-10T22:00:00-03:00", "summary": "<p>Ragnar gathers a crew in preparation for an epic journey; the earl is overcome with paranoia.</p>", "runtime": 60, "airdate": "2013-03-10", "airtime": "22:00"}, {"name": "Dispossessed", "number": 3, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1549"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/1549/vikings-1x03-dispossessed", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/8/21465.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/8/21465.jpg"}, "id": 1549, "airstamp": "2013-03-17T22:00:00-03:00", "summary": "<p>Ragnar and his crew raid a monastery, securing a bounty of riches that impresses the earl.</p>", "runtime": 60, "airdate": "2013-03-17", "airtime": "22:00"}, {"name": "Trial", "number": 4, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1550"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/1550/vikings-1x04-trial", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/8/21468.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/8/21468.jpg"}, "id": 1550, "airstamp": "2013-03-24T22:00:00-03:00", "summary": "<p>The Vikings return to England; the earl plants a spy within Ragnar's crew.</p>", "runtime": 60, "airdate": "2013-03-24", "airtime": "22:00"}, {"name": "Raid", "number": 5, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1551"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/1551/vikings-1x05-raid", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/8/21466.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/8/21466.jpg"}, "id": 1551, "airstamp": "2013-03-31T22:00:00-03:00", "summary": "<p>A prophet predicts the earl's future; Ragnar's home and farm are destroyed.</p>", "runtime": 60, "airdate": "2013-03-31", "airtime": "22:00"}, {"name": "Burial of the Dead", "number": 6, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1552"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/1552/vikings-1x06-burial-of-the-dead", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/8/21467.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/8/21467.jpg"}, "id": 1552, "airstamp": "2013-04-07T22:00:00-03:00", "summary": "<p>Ragnar faces off with the Earl after learning of Rollo's torture.</p>", "runtime": 60, "airdate": "2013-04-07", "airtime": "22:00"}, {"name": "A King's Ransom", "number": 7, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1553"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/1553/vikings-1x07-a-kings-ransom", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/8/21472.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/8/21472.jpg"}, "id": 1553, "airstamp": "2013-04-14T22:00:00-03:00", "summary": "<p>Ragnar presents a proposal to King Aelle.</p>", "runtime": 60, "airdate": "2013-04-14", "airtime": "22:00"}, {"name": "Sacrifice", "number": 8, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1554"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/1554/vikings-1x08-sacrifice", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/8/21469.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/8/21469.jpg"}, "id": 1554, "airstamp": "2013-04-21T22:00:00-03:00", "summary": "<p>Ragnar attempts to cope with the death of his unborn son; Lagertha seeks answers from the gods.</p>", "runtime": 60, "airdate": "2013-04-21", "airtime": "22:00"}, {"name": "All Change", "number": 9, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1555"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/1555/vikings-1x09-all-change", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/8/21470.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/8/21470.jpg"}, "id": 1555, "airstamp": "2013-04-28T22:00:00-03:00", "summary": "<p>Ragnar attempts to settle a land dispute; a plague breaks out.</p>", "runtime": 60, "airdate": "2013-04-28", "airtime": "22:00"}, {"name": "Brother's War", "number": 1, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1556"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/1556/vikings-2x01-brothers-war", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/44/111364.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/44/111364.jpg"}, "id": 1556, "airstamp": "2014-02-27T22:00:00-04:00", "summary": "<p>A battle begins, pitting Ragnar and King Horik's forces against Jarl Borg and Rollo in the Season 2 premiere. Meanwhile, Princess Aslaug arrives in Kattegat with a surprise that leaves Ragnar in a difficult predicament.</p>", "runtime": 60, "airdate": "2014-02-27", "airtime": "22:00"}, {"name": "Invasion", "number": 2, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1557"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/1557/vikings-2x02-invasion", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/44/111366.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/44/111366.jpg"}, "id": 1557, "airstamp": "2014-03-06T22:00:00-04:00", "summary": "<p>An unlikely alliance plans a raid on England; Ragnar and his men face off against a formidable foe.</p>", "runtime": 60, "airdate": "2014-03-06", "airtime": "22:00"}, {"name": "Treachery", "number": 3, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1558"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/1558/vikings-2x03-treachery", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/44/111367.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/44/111367.jpg"}, "id": 1558, "airstamp": "2014-03-13T22:00:00-03:00", "summary": "<p>King Ecbert faces a new foe; Ragnar races to dominate in the West.</p>", "runtime": 60, "airdate": "2014-03-13", "airtime": "22:00"}, {"name": "Eye For an Eye", "number": 4, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1559"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/1559/vikings-2x04-eye-for-an-eye", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/44/111368.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/44/111368.jpg"}, "id": 1559, "airstamp": "2014-03-20T22:00:00-03:00", "summary": "<p>Ragnar and Ecbert meet face-to-face; Rollo is forced to become a leader.</p>", "runtime": 60, "airdate": "2014-03-20", "airtime": "22:00"}, {"name": "Answers in Blood", "number": 5, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1560"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/1560/vikings-2x05-answers-in-blood", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/44/111369.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/44/111369.jpg"}, "id": 1560, "airstamp": "2014-03-27T22:00:00-03:00", "summary": "<p>Lagertha and Ragnar unite to battle Jarl Borg. Later, Bjorn makes a difficult choice.</p>", "runtime": 60, "airdate": "2014-03-27", "airtime": "22:00"}, {"name": "Unforgiven", "number": 6, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1561"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/1561/vikings-2x06-unforgiven", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/44/111370.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/44/111370.jpg"}, "id": 1561, "airstamp": "2014-04-03T22:00:00-03:00", "summary": "<p>King Horik presents a surprising proposition to Ragnar; Athelstan becomes confidant to King Ecbert.</p>", "runtime": 60, "airdate": "2014-04-03", "airtime": "22:00"}, {"name": "Blood Eagle", "number": 7, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1562"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/1562/vikings-2x07-blood-eagle", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/44/111371.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/44/111371.jpg"}, "id": 1562, "airstamp": "2014-04-10T22:00:00-03:00", "summary": "<p>Ragnar and King Horik clash over how to punish Jarl Borg. Meanwhile, Ecbert eyes a new alliance.</p>", "runtime": 60, "airdate": "2014-04-10", "airtime": "22:00"}, {"name": "Boneless", "number": 8, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1563"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/1563/vikings-2x08-boneless", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/44/111372.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/44/111372.jpg"}, "id": 1563, "airstamp": "2014-04-17T22:00:00-03:00", "summary": "<p>Aslaug prepares to give birth; Ragnar, Horik and Lagertha set sail for Wessex.</p>", "runtime": 60, "airdate": "2014-04-17", "airtime": "22:00"}, {"name": "The Choice", "number": 9, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1564"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/1564/vikings-2x09-the-choice", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/44/111373.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/44/111373.jpg"}, "id": 1564, "airstamp": "2014-04-24T22:00:00-03:00", "summary": "<p>Ragnar's men are engaged in battle while marching to King Ecbert's villa.</p>", "runtime": 60, "airdate": "2014-04-24", "airtime": "22:00"}, {"name": "The Lord's Prayer", "number": 10, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1565"}}, "season": 2, "url": "http://www.tvmaze.com/episodes/1565/vikings-2x10-the-lords-prayer", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/44/111374.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/44/111374.jpg"}, "id": 1565, "airstamp": "2014-05-01T22:00:00-03:00", "summary": "<p>Ragnar and King Horik return to Kattegat in the Series 2 finale. Meanwhile, new alliances are formed.</p>", "runtime": 60, "airdate": "2014-05-01", "airtime": "22:00"}, {"name": "Mercenary", "number": 1, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/92004"}}, "season": 3, "url": "http://www.tvmaze.com/episodes/92004/vikings-3x01-mercenary", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/44/111672.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/44/111672.jpg"}, "id": 92004, "airstamp": "2015-02-19T22:00:00-04:00", "summary": "<p>King Ecbert proposes a deal to the Vikings in the Season 3 premiere. Later, Ragnar leads his forces into battle.</p>", "runtime": 60, "airdate": "2015-02-19", "airtime": "22:00"}, {"name": "The Wanderer", "number": 2, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/120600"}}, "season": 3, "url": "http://www.tvmaze.com/episodes/120600/vikings-3x02-the-wanderer", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/44/111686.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/44/111686.jpg"}, "id": 120600, "airstamp": "2015-02-26T22:00:00-04:00", "summary": "<p>Lagertha and Athelstan help to establish the Viking settlement in Wessex. Harbard, a mysterious wanderer, arrives in Kattegat, realizing the forewarned dreams of Aslaug, Siggy and Helga.</p>", "runtime": 60, "airdate": "2015-02-26", "airtime": "22:00"}, {"name": "Warrior's Fate", "number": 3, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/140710"}}, "season": 3, "url": "http://www.tvmaze.com/episodes/140710/vikings-3x03-warriors-fate", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/44/111954.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/44/111954.jpg"}, "id": 140710, "airstamp": "2015-03-05T22:00:00-04:00", "summary": "<p>King Ecbert visits the developing Viking settlement as the first harvest is sown and rebuffs criticism from his leading nobles. In Mercia, the Wessex/Viking forces charge at the battle at the Hill of the Ash. In Kattegat, Harbard arrives in to the Great Hall and his mysterious aura fascinates Aslaug and Helga but Siggy remains suspicious. There are strange and tragic happenings in Kattegat, and Siggy suspects Harbard is the cause.<br /></p>", "runtime": 60, "airdate": "2015-03-05", "airtime": "22:00"}, {"name": "Scarred", "number": 4, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/140711"}}, "season": 3, "url": "http://www.tvmaze.com/episodes/140711/vikings-3x04-scarred", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/44/112382.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/44/112382.jpg"}, "id": 140711, "airstamp": "2015-03-12T22:00:00-03:00", "summary": "<p>The victorious Wessex/Viking forces return to Wessex but there is rancour in the Viking camp\u2013Floki is angry over the alliance with Ecbert and resentful the influence that he feels Athelstan has over Ragnar. Princess Kwenthrith makes some calculating political moves following the battle at the Hill of the Ash in Mercia. Visitors from the past arrive in Hederby, at Kalf's invitation.</p>", "runtime": 60, "airdate": "2015-03-12", "airtime": "22:00"}, {"name": "The Usurper", "number": 5, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/140712"}}, "season": 3, "url": "http://www.tvmaze.com/episodes/140712/vikings-3x05-the-usurper", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/45/112689.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/45/112689.jpg"}, "id": 140712, "airstamp": "2015-03-19T22:00:00-03:00", "summary": "<p>The fleet returns to Kattegat to find tragic circumstances await. Lagertha learns that her Earldom has been usurped and convinces Ragnar to travel to Hederby with her in an effort to reclaim her title. Ragnar causes great surprise when he announces the next raid. The Seer has interesting prophecies for Rollo.</p>", "runtime": 60, "airdate": "2015-03-19", "airtime": "22:00"}, {"name": "Born Again", "number": 6, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/140713"}}, "season": 3, "url": "http://www.tvmaze.com/episodes/140713/vikings-3x06-born-again", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/45/112707.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/45/112707.jpg"}, "id": 140713, "airstamp": "2015-03-26T22:00:00-03:00", "summary": "<p>Preparations for the Paris raid pick up pace, and Rollo ponders the Seer's prophecy about his destiny. Kalf arrives in Kattegat to join the raid and the Viking fleet sails up the Seine and catches sight of Paris for the first time. Porunn and Judith both deliver children, and with new life there is also death.</p>", "runtime": 60, "airdate": "2015-03-26", "airtime": "22:00"}, {"name": "Paris", "number": 7, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/145578"}}, "season": 3, "url": "http://www.tvmaze.com/episodes/145578/vikings-3x07-paris", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/45/112972.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/45/112972.jpg"}, "id": 145578, "airstamp": "2015-04-02T22:00:00-03:00", "summary": "<p>The appearance of the Viking fleet causes panic among the population of Paris. The Emperor Charles, urged by his daughter Gisla, announces in Court he will remain in the city. The defense of Paris is in the hands of its Commander, Count Odo, who hopes that a heroic victory against the Vikings will gain him Gisla's hand in marriage. The Vikings establish camp downriver from the city and plan their attack.</p>", "runtime": 60, "airdate": "2015-04-02", "airtime": "22:00"}, {"name": "To the Gates!", "number": 8, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/145579"}}, "season": 3, "url": "http://www.tvmaze.com/episodes/145579/vikings-3x08-to-the-gates", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/45/112989.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/45/112989.jpg"}, "id": 145579, "airstamp": "2015-04-09T22:00:00-03:00", "summary": "<p>The Viking army embarks and Paris goes into lockdown as the army prepares the defense, overseen by Odo. The Vikings are determined to make it through the gates of Paris and will stop at nothing to succeed.</p>", "runtime": 60, "airdate": "2015-04-09", "airtime": "22:00"}, {"name": "Breaking Point", "number": 9, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/145580"}}, "season": 3, "url": "http://www.tvmaze.com/episodes/145580/vikings-3x09-breaking-point", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/45/114097.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/45/114097.jpg"}, "id": 145580, "airstamp": "2015-04-16T22:00:00-03:00", "summary": "<p>The Vikings go all out for a second assault on Paris. Recognizing the desperate state of affairs for his city, Emperor Charles must make a difficult decision. In Wessex, Judith is compelled to make a difficult decision of her own.</p>", "runtime": 60, "airdate": "2015-04-16", "airtime": "22:00"}, {"name": "The Dead", "number": 10, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/151525"}}, "season": 3, "url": "http://www.tvmaze.com/episodes/151525/vikings-3x10-the-dead", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/45/114118.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/45/114118.jpg"}, "id": 151525, "airstamp": "2015-04-23T22:00:00-03:00", "summary": "<p>With one last chance to take Paris, Ragnar and his Viking troops take a daring chance. Ragnar asks Bjorn for a favor that could change the course of Viking history.</p>", "runtime": 60, "airdate": "2015-04-23", "airtime": "22:00"}, {"name": "A Good Treason", "number": 1, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/536749"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/536749/vikings-4x01-a-good-treason", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/45/113398.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/45/113398.jpg"}, "id": 536749, "airstamp": "2016-02-18T22:00:00-04:00", "summary": "<p>Entry to Valhalla eludes Ragnar but as he lies in his sick bed in Kattegat events unfold beyond his control. Bjorn orders the arrest of Floki for Athelstan's murder and in Paris, Rollo betrays his allies to strengthen his position. Meanwhile, in Hedeby, Kalf announces joint Earldom with Lagertha, but is Lagertha actually willing to share what once was hers?</p>", "runtime": 60, "airdate": "2016-02-18", "airtime": "22:00"}, {"name": "Kill the Queen", "number": 2, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/546170"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/546170/vikings-4x02-kill-the-queen", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/46/116604.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/46/116604.jpg"}, "id": 546170, "airstamp": "2016-02-25T22:00:00-04:00", "summary": "<p>Floki hopes to escape from Ragnar and Kattegat. Rollo makes great efforts in Paris to win over his new bride, Princess Gisla. King Ecbert orders his son Aethelwulf to rescue Queen Kwenthrith from opposing forces in Mercia.</p>", "runtime": 60, "airdate": "2016-02-25", "airtime": "22:00"}, {"name": "Mercy", "number": 3, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/546171"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/546171/vikings-4x03-mercy", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/40/100831.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/40/100831.jpg"}, "id": 546171, "airstamp": "2016-03-03T22:00:00-04:00", "summary": "<p>Floki faces the wrath of Ragnar. Intrigue prevails in the French Court as Rollo finds an unlikely ally. Kalf and Erlendur plot their ascent. Bjorn must fight a fierce opponent in the wilderness.</p>", "runtime": 60, "airdate": "2016-03-03", "airtime": "22:00"}, {"name": "Yol", "number": 4, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/546172"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/546172/vikings-4x04-yol", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/40/100833.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/40/100833.jpg"}, "id": 546172, "airstamp": "2016-03-10T22:00:00-04:00", "summary": "<p>The Yule Log burns in the Great Hall in Kattegat and mysterious visitors are welcomed, among them a Norwegian King. King Aelle visits Wessex for Christmas and is concerned with how family relations are developing. In Paris, Rollo's future hangs in the balance - can he win over his new bride, Gisla, as a Papal Delegation arrives to arrange a divorce?</p>", "runtime": 60, "airdate": "2016-03-10", "airtime": "22:00"}, {"name": "Promised", "number": 5, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/618779"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/618779/vikings-4x05-promised", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/49/123323.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/49/123323.jpg"}, "id": 618779, "airstamp": "2016-03-17T22:00:00-03:00", "summary": "<p>There's an air of betrayal at Court in Paris and in Wessex. Ecbert agrees to support Kwenthrith in Mercia, but is Ecbert a true ally? Ragnar confides in Yidu his darkest secret. Happiness comes to Lagertha and Kalf and a marriage is arranged.</p>", "runtime": 60, "airdate": "2016-03-17", "airtime": "22:00"}, {"name": "What Might Have Been", "number": 6, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/618780"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/618780/vikings-4x06-what-might-have-been", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/50/125401.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/50/125401.jpg"}, "id": 618780, "airstamp": "2016-03-24T22:00:00-03:00", "summary": "<p>Ragnar announces a raid on Paris at the Thing in Kattegat. The fleet is battered by a storm after it sets sail, but the greatest challenge for the Vikings lies ahead in Paris, where Rollo awaits. Meanwhile, Ecbert dispatches Aethelwulf and Alfred on a pilgrimage to Rome.</p>", "runtime": 60, "airdate": "2016-03-24", "airtime": "22:00"}, {"name": "The Profit and the Loss", "number": 7, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/632380"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/632380/vikings-4x07-the-profit-and-the-loss", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/50/126316.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/50/126316.jpg"}, "id": 632380, "airstamp": "2016-03-31T22:00:00-03:00", "summary": "<p>On the Seine, the Vikings attack with considerable force but can they breach Rollo's defenses? Meanwhile, Harbard, the mysterious wanderer, returns and causes a stir amongst the women of Kattegat.</p>", "runtime": 60, "airdate": "2016-03-31", "airtime": "22:00"}, {"name": "Portage", "number": 8, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/648584"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/648584/vikings-4x08-portage", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/51/129427.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/51/129427.jpg"}, "id": 648584, "airstamp": "2016-04-07T22:00:00-03:00", "summary": "<p>The Vikings call Ragnar's leadership into question. Ragnar himself remains inscrutable and orders the fleet to beach at a cliff face where he unveils an ingenious plan. Rollo and Gisla have news that strengthens Rollo's position at the French Court.<p></p></p>", "runtime": 60, "airdate": "2016-04-07", "airtime": "22:00"}, {"name": "Death All 'Round", "number": 9, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/648585"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/648585/vikings-4x09-death-all-round", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/53/132702.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/53/132702.jpg"}, "id": 648585, "airstamp": "2016-04-14T22:00:00-03:00", "summary": "<p>The labours of the Vikings bear fruit as, within sight of Paris, they re-launch their fleet but this time with some formidable new structures that Floki has engineered. Aethelwulf and Alfred arrive in Rome and honours are conferred by Pope Leo. Relations are not so cordial in Wessex.</p>", "runtime": 60, "airdate": "2016-04-14", "airtime": "22:00"}, {"name": "The Last Ship", "number": 10, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/648586"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/648586/vikings-4x10-the-last-ship", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/54/135333.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/54/135333.jpg"}, "id": 648586, "airstamp": "2016-04-21T22:00:00-03:00", "summary": "<p>A fierce battle between the Vikings and the French eventually comes down to Ragnar against Rollo and the outcome seals the fates of the two brothers.</p>", "runtime": 60, "airdate": "2016-04-21", "airtime": "22:00"}, {"name": "The Outsider", "number": 11, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/928734"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/928734/vikings-4x11-the-outsider", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/82/206241.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/82/206241.jpg"}, "id": 928734, "airstamp": "2016-11-30T21:00:00-04:00", "summary": "<p>Ragnar has returned to Kattegat and there are secrets to be revealed. He makes plans to return to Wessex to right past wrongs but finds that those who were once his closest allies now have plans of their own. Even the Lothbrok sons refuse to join their father's mission \u2013 except one.</p>", "runtime": 60, "airdate": "2016-11-30", "airtime": "21:00"}, {"name": "The Vision", "number": 12, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/976050"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/976050/vikings-4x12-the-vision", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/82/206230.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/82/206230.jpg"}, "id": 976050, "airstamp": "2016-12-07T21:00:00-04:00", "summary": "<p>The Kattegat locals chide Ragnar that the gods have deserted him as he struggles to crew his voyage to Wessex. It's Bjorn who has charisma now as he prepares to fulfill his long-held dream to explore the Mediterranean in a sleek new boat built for him by Floki. By contrast, Ragnar must accept weathered boats and a crew he has bribed but when Aslaug foresees that the fleet will be lost in a vicious storm her warnings go un-heeded by Ragnar and her precious son, Ivar.</p>", "runtime": 60, "airdate": "2016-12-07", "airtime": "21:00"}, {"name": "Two Journeys", "number": 13, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/976051"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/976051/vikings-4x13-two-journeys", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/82/206229.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/82/206229.jpg"}, "id": 976051, "airstamp": "2016-12-14T21:00:00-04:00", "summary": "<p>Ragnar's fleet is shipwrecked and washes up on the Wessex coast, but without enough survivors to form a credible force, Ragnar and Ivar have a difficult decision to make. Rollo meets his Viking comrades again and can't resist the urge to join Bjorn's voyage, risking his marriage and his new Frankish identity. Ragnar too is taking a risk as he carries Ivar towards the Royal Villa in Wessex \u2013 throwing himself and his son upon the mercy of King Ecbert.</p>", "runtime": 60, "airdate": "2016-12-14", "airtime": "21:00"}, {"name": "In the Uncertain Hour Before the Morning", "number": 14, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/981182"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/981182/vikings-4x14-in-the-uncertain-hour-before-the-morning", "image": null, "id": 981182, "airstamp": "2016-12-21T21:00:00-04:00", "summary": "", "runtime": 60, "airdate": "2016-12-21", "airtime": "21:00"}, {"name": "All His Angels", "number": 15, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/981183"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/981183/vikings-4x15-all-his-angels", "image": null, "id": 981183, "airstamp": "2016-12-28T21:00:00-04:00", "summary": "", "runtime": 60, "airdate": "2016-12-28", "airtime": "21:00"}, {"name": "Crossings", "number": 16, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1003294"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/1003294/vikings-4x16-crossings", "image": null, "id": 1003294, "airstamp": "2017-01-04T21:00:00-04:00", "summary": null, "runtime": 60, "airdate": "2017-01-04", "airtime": "21:00"}, {"name": "The Great Army", "number": 17, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1003295"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/1003295/vikings-4x17-the-great-army", "image": null, "id": 1003295, "airstamp": "2017-01-11T21:00:00-04:00", "summary": null, "runtime": 60, "airdate": "2017-01-11", "airtime": "21:00"}, {"name": "Revenge", "number": 18, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1003296"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/1003296/vikings-4x18-revenge", "image": null, "id": 1003296, "airstamp": "2017-01-18T21:00:00-04:00", "summary": null, "runtime": 60, "airdate": "2017-01-18", "airtime": "21:00"}, {"name": "On the Eve", "number": 19, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1003297"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/1003297/vikings-4x19-on-the-eve", "image": null, "id": 1003297, "airstamp": "2017-01-25T21:00:00-04:00", "summary": null, "runtime": 60, "airdate": "2017-01-25", "airtime": "21:00"}, {"name": "The Reckoning", "number": 20, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/1006531"}}, "season": 4, "url": "http://www.tvmaze.com/episodes/1006531/vikings-4x20-the-reckoning", "image": null, "id": 1006531, "airstamp": "2017-02-01T21:00:00-04:00", "summary": null, "runtime": 60, "airdate": "2017-02-01", "airtime": "21:00"}]
[{"name": "Wolferton Splash", "number": 1, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/560876"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/560876/the-crown-1x01-wolferton-splash", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/83/208925.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/83/208925.jpg"}, "id": 560876, "airstamp": "2016-11-04T12:00:00+00:00", "summary": "<p>A young Princess Elizabeth marries Prince Philip. As King George VI's health worsens, Winston Churchill is elected prime minister for the second time.</p>", "runtime": 60, "airdate": "2016-11-04", "airtime": ""}, {"name": "Hyde Park Corner", "number": 2, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/731302"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/731302/the-crown-1x02-hyde-park-corner", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/83/208926.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/83/208926.jpg"}, "id": 731302, "airstamp": "2016-11-04T12:00:00+00:00", "summary": "<p>With King George too ill to travel, Elizabeth and Philip embark on a four-continent Commonwealth tour. Party leaders attempt to undermine Churchill.</p>", "runtime": 60, "airdate": "2016-11-04", "airtime": ""}, {"name": "Windsor", "number": 3, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/731303"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/731303/the-crown-1x03-windsor", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/83/208927.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/83/208927.jpg"}, "id": 731303, "airstamp": "2016-11-04T12:00:00+00:00", "summary": "<p>With Elizabeth in new role, Philip tries to assert some power. Churchill wants to delay the coronation. King George's disgraced brother arrives.</p>", "runtime": 60, "airdate": "2016-11-04", "airtime": ""}, {"name": "Act of God", "number": 4, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/731304"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/731304/the-crown-1x04-act-of-god", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/83/208982.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/83/208982.jpg"}, "id": 731304, "airstamp": "2016-11-04T12:00:00+00:00", "summary": "<p>When dense smog cripples London for days and creates a serious health hazard, Churchill's inaction leaves him vulnerable to his political enemies.</p>", "runtime": 60, "airdate": "2016-11-04", "airtime": ""}, {"name": "Smoke and Mirrors", "number": 5, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/731305"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/731305/the-crown-1x05-smoke-and-mirrors", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/83/208983.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/83/208983.jpg"}, "id": 731305, "airstamp": "2016-11-04T12:00:00+00:00", "summary": "<p>Elizabeth rejects protocol by appointing Philip to coordinate her coronation, but his ideas create conflict. The Duke of Windsor returns to London.</p>", "runtime": 60, "airdate": "2016-11-04", "airtime": ""}, {"name": "Gelignite", "number": 6, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/731306"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/731306/the-crown-1x06-gelignite", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/83/208985.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/83/208985.jpg"}, "id": 731306, "airstamp": "2016-11-04T12:00:00+00:00", "summary": "<p>Margaret and Peter come to Elizabeth with a request. With a royal scandal about to break in the newspapers, the Queen Mother intervenes.</p>", "runtime": 60, "airdate": "2016-11-04", "airtime": ""}, {"name": "Scientia Potentia Est", "number": 7, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/731307"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/731307/the-crown-1x07-scientia-potentia-est", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/83/208987.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/83/208987.jpg"}, "id": 731307, "airstamp": "2016-11-04T12:00:00+00:00", "summary": "<p>As the Soviets test the H-bomb, both Churchill and Eden have major health crises. Angry about her inadequate education, Elizabeth hires a tutor.</p>", "runtime": 60, "airdate": "2016-11-04", "airtime": ""}, {"name": "Pride and Joy", "number": 8, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/731308"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/731308/the-crown-1x08-pride-and-joy", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/83/208988.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/83/208988.jpg"}, "id": 731308, "airstamp": "2016-11-04T12:00:00+00:00", "summary": "<p>Elizabeth and Philip go on an exhausting world journey. Margaret takes over some of her sister's official duties and tries to liven things up.</p>", "runtime": 60, "airdate": "2016-11-04", "airtime": ""}, {"name": "Assassins", "number": 9, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/731309"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/731309/the-crown-1x09-assassins", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/83/208989.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/83/208989.jpg"}, "id": 731309, "airstamp": "2016-11-04T12:00:00+00:00", "summary": "<p>As tensions with Philip increase, Elizabeth spends time with her old friend Porchey. Churchill's portrait is painted for his 80th birthday.</p>", "runtime": 60, "airdate": "2016-11-04", "airtime": ""}, {"name": "Gloriana", "number": 10, "_links": {"self": {"href": "http://api.tvmaze.com/episodes/731310"}}, "season": 1, "url": "http://www.tvmaze.com/episodes/731310/the-crown-1x10-gloriana", "image": {"original": "http://tvmazecdn.com/uploads/images/original_untouched/83/208990.jpg", "medium": "http://tvmazecdn.com/uploads/images/medium_landscape/83/208990.jpg"}, "id": 731310, "airstamp": "2016-11-04T12:00:00+00:00", "summary": "<p>Margaret and Peter are reunited, but another obstacle stands in their way. Elizabeth is torn between her duty as Queen and her love for her sister.</p>", "runtime": 60, "airdate": "2016-11-04", "airtime": ""}]